extends ../layouts/base

block content
    h1 Hello World

    nav(aria-label='...')
        ul.pagination
            li.page-item.disabled
                a.page-link(href='#', tabindex='-1', aria-disabled='true') Previous
            li.page-item
                a.page-link(href='#') 1
            li.page-item.active(aria-current='page')
                a.page-link(href='#')
                    | 2
                    span.sr-only (current)
            li.page-item
                a.page-link(href='#') 3
            li.page-item
                a.page-link(href='#') Next
